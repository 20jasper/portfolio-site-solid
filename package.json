{
	"name": "portfolio-site-solid",
	"version": "0.0.0",
	"description": "",
	"type": "module",
	"scripts": {
		"build": "astro build",
		"start": "astro dev",
		"start:prod": "pnpm run build && astro preview",
		"verify-types": "tsc --noEmit",
		"test": "vitest",
		"lint": "pnpm run lint:eslint && pnpm run lint:astro && pnpm run lint:style",
		"lint:fix": "pnpm run lint:eslint:fix && pnpm run lint:style:fix",
		"lint:ci": "pnpm run lint:eslint:ci && pnpm run lint:astro",
		"lint:astro": "astro check",
		"lint:style": "stylelint \"**/*.css\" --allow-empty-input --cache",
		"lint:style:fix": "stylelint \"**/*.css\" --allow-empty-input --cache --fix",
		"lint:style:ci": "stylelint \"**/*.css\" --allow-empty-input --max-warnings=0",
		"lint:eslint": "eslint ./src/",
		"lint:eslint:fix": "eslint ./src/ --fix",
		"lint:eslint:ci": "eslint ./src/ --max-warnings=0",
		"format": "prettier ./ --write --cache",
		"format:check": "prettier ./ --check --cache"
	},
	"devDependencies": {
		"@solidjs/testing-library": "^0.7.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@types/testing-library__jest-dom": "^5.14.6",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"@typescript-eslint/parser": "^5.59.11",
		"eslint": "^8.43.0",
		"eslint-plugin-astro": "^0.27.1",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-only-warn": "^1.1.0",
		"eslint-plugin-solid": "^0.12.1",
		"jsdom": "^22.1.0",
		"prettier": "^2.8.8",
		"prettier-plugin-astro": "^0.10.0",
		"stylelint": "^15.10.1",
		"stylelint-config-idiomatic-order": "^9.0.0",
		"stylelint-config-standard": "^33.0.0",
		"stylelint-csstree-validator": "^3.0.0",
		"stylelint-no-unsupported-browser-features": "^6.1.0",
		"stylelint-plugin-logical-css": "^0.12.0",
		"stylelint-rem-over-px": "^1.0.0",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vite-plugin-solid": "^2.7.0",
		"vitest": "^0.32.0"
	},
	"dependencies": {
		"@astrojs/sitemap": "^1.3.3",
		"@astrojs/ts-plugin": "^1.0.10",
		"astro": "^2.6.4",
		"solid-js": "^1.7.6"
	}
}
